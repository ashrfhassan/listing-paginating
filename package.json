  "scripts": {
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "storybook": "start-storybook -p 6006 -s ./src/stories/assets",
    "build-storybook": "build-storybook -o docs-build -s ./src/stories/assets",
    "build": "webpack --mode production --config webpack.config.js && npm run build:path",
    "build:path": "tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "test": "jest --u",
    "prepare": "husky install && npm run build",
    "prepublishOnly": "npm test && npm run lint && npm run format && npm run build",
    "preversion": "npm run lint && npm run format",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
